    <div class="clearfix"></div>
    <div class="col-md-30">
      <div class="tile">

        <table class="table table-bordered">
          <thead>
            <tr>
              <th>#</th>
              <th>Name</th>
              <th>Email</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(i=0;i<data.length;i++) {%>
              <tr>
                <td>
                  <%= i+1%>
                </td>
                <td>
                  <%= data[i].name%>
                </td>
                <td>
                  <%= data[i].email%>
                </td>
                <%if(data[i].status==="0"){%>
                <td> <a href="/admin/approve-user/<%= data[i]._id%>">
                  <button class="btn btn-info"  type="button">Approve</button>
                </a><a href="/admin/delete-user/<%= data[i]._id%>">
                  <button class="btn btn-danger" type="button">detele</button></a></td>
                <%}else{%>
                  <td><button class="btn btn-info" type="button">Approved</button><a href="/admin/approve-user/<%= data[i]._id%>">
                    <button class="btn btn-danger" type="button">detele</button></a></td>
                  <%}%>
              
              </tr>
              <%}%>

          </tbody>
        </table>
      </div>
